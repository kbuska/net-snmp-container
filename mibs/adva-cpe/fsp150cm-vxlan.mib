    STATUS      current
    DESCRIPTION
           "This object specify inner vlanId."
    ::= { f3VTEPEntry 11 }

f3VTEPInnerVlanPri OBJECT-TYPE
    SYNTAX      VlanPriority
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
           "This object specify inner vlan priority."
    ::= { f3VTEPEntry 12 }

f3VTEPDSCPValue OBJECT-TYPE
    SYNTAX      Unsigned32 (0..63)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
           "This object specify DSCP value."
    ::= { f3VTEPEntry 13 }

f3VTEPDestUDPPort OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
           "This object specify destination UDP port value."
    ::= { f3VTEPEntry 14 }

f3VTEPStorageType OBJECT-TYPE
    SYNTAX      StorageType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
           "The type of storage configured for this entry."
    ::= { f3VTEPEntry 15 }

f3VTEPRowStatus OBJECT-TYPE
    SYNTAX      RowStatus
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
           "The status of this row.
          	An entry MUST NOT exist in the active state unless all
          	objects in the entry have an appropriate value, as described
          	in the description clause for each writable object.
          	
          	The values of f3VTEPRowStatus supported are
          	createAndGo(4) and destroy(6).  All mandatory attributes
          	must be specified in a single SNMP SET request with
          	f3VTEPRowStatus value as createAndGo(4).
          	Upon successful row creation, this object has a
          	value of active(1).
          	
          	The f3VTEPRowStatus object may be modified if
          	the associated instance of this object is equal to active(1)."
    ::= { f3VTEPEntry 16 }

f3VTEPAction OBJECT-TYPE
    SYNTAX      VtepAction
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
           "The VTEP action, includes retrieve route.
           Note: before get data from f3VTEPIpRouteTable, need to set this action firstly "
    ::= { f3VTEPEntry 17 }

f3VTEPPingIpv4Destination OBJECT-TYPE
