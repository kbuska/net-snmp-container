F3-VXLAN-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-COMPLIANCE, OBJECT-GROUP 
             FROM SNMPv2-CONF
    MODULE-IDENTITY, OBJECT-TYPE, Integer32, Unsigned32, IpAddress, Counter64
             FROM SNMPv2-SMI
    RowStatus, StorageType, TruthValue,DisplayString,
    VariablePointer, TEXTUAL-CONVENTION, MacAddress,
    DateAndTime
             FROM SNMPv2-TC
    AdminState, OperationalState, SecondaryState,
    VlanId, VlanPriority,IpVersion,
    CmPmIntervalType, CmPmBinAction, PerfCounter64
             FROM  CM-COMMON-MIB
    Ipv6Address
             FROM  IPV6-TC
    neIndex
             FROM CM-ENTITY-MIB
    IpMode, IpEntryType
             FROM  CM-IP-MIB
    TrafficIpRouteStatus
             FROM F3-L3-MIB
    fsp150cm
             FROM  ADVA-MIB; 

f3VXLANMIB MODULE-IDENTITY
    LAST-UPDATED        "201508140000Z"
    ORGANIZATION    "ADVA Optical Networking SE"
    CONTACT-INFO
        "Web URL: http://adva.com/
        E-mail:  support@adva.com
        Postal:  ADVA Optical Networking SE
             Campus Martinsried
             Fraunhoferstrasse 9a
             82152 Martinsried/Munich
             Germany
        Phone: +49 089 89 06 65 0
        Fax:  +49 089 89 06 65 199 "
    DESCRIPTION
            "This module defines the Ethernet Service Manager MIB defitinions
             used by the F3 (FSP150CM/CC) product lines.  
             Copyright (C) ADVA."
    REVISION        "201508140000Z"
    DESCRIPTION
        "
         Notes from release 201508140000Z." 

    ::= {fsp150cm 45}

-- 
-- OID definitions
-- 
f3VXLANConfigObjects      OBJECT IDENTIFIER ::= {f3VXLANMIB 1}
f3VXLANPerformance        OBJECT IDENTIFIER ::= {f3VXLANMIB 2}
f3VXLANNotifications      OBJECT IDENTIFIER ::= {f3VXLANMIB 3}
f3VXLANConformance        OBJECT IDENTIFIER ::= {f3VXLANMIB 4}

-- 
-- Textual Conventions 
-- 
VtepVxlanMode ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION
        "The vtep vxlan mode."
    SYNTAX       INTEGER {
                   notApplicable(0),
                   multicast(1),
                   unicast(2)
                 }

VtepAction ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION
        "The vtep action."
    SYNTAX       INTEGER {
                   notApplicable(0),
                   retrieveRoute(1),
                   ping(2),
                   traceroute(3)
                 }

VxlanSegmentAction ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION
        "The vxlan segment action."
    SYNTAX       INTEGER {
                   notApplicable(0),
                   retrieveDMac2DIP(1),
                   flushDmac2DIP(2)
                 }

VxlanDmac2DipTableFullAction ::= TEXTUAL-CONVENTION
    STATUS       current
    DESCRIPTION
        "The vxlan segment dmac to dip table full action."
    SYNTAX       INTEGER {
                   forward(1),
                   discard(2)
                 }

--
-- VXLAN Config Table
--
f3VTEPTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF F3VTEPEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table has list of entries with which FSPNM can associate some
         information in the form of name-value pairs.
         NOTE: This table is for FSPNM use only."
   ::= { f3VXLANConfigObjects 1 }

f3VTEPEntry OBJECT-TYPE
    SYNTAX      F3VTEPEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The conceptual row in f3VTEPTable."
    INDEX { neIndex, f3VTEPIndex }
   ::= { f3VTEPTable 1 }

F3VTEPEntry ::= SEQUENCE {
    f3VTEPIndex                                       Unsigned32,
    f3VTEPAdminState                                  AdminState,
    f3VTEPSecondaryState                              SecondaryState,
    f3VTEPOperationalState                            OperationalState,
    f3VTEPOuterVlanControl                            TruthValue,
    f3VTEPOuterVlanEtherType                          Unsigned32,
    f3VTEPOuterVlanId                                 VlanId,
    f3VTEPOuterVlanPri                                VlanPriority,
    f3VTEPInnerVlanControl                            TruthValue,
    f3VTEPInnerVlanEtherType                          Unsigned32,
    f3VTEPInnerVlanId                                 VlanId,
    f3VTEPInnerVlanPri                                VlanPriority,
    f3VTEPDSCPValue                                   Unsigned32,
    f3VTEPDestUDPPort                                 Unsigned32,
    f3VTEPStorageType                                 StorageType,
    f3VTEPRowStatus                                   RowStatus,
    f3VTEPAction                                      VtepAction,
    f3VTEPPingIpv4Destination                         IpAddress,
    f3VTEPTraceRouteIpv4Destination                   IpAddress,
    f3VTEPPingResult                                  DisplayString,
    f3VTEPTraceRouteResult                            DisplayString,
    f3VTEPAlias                                       DisplayString,
    f3VTEPVxlanMode                                   VtepVxlanMode
}

f3VTEPIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
           "This object unique identify one VXLAN tunnel end point."
    ::= { f3VTEPEntry 1 }

f3VTEPAdminState OBJECT-TYPE
    SYNTAX      AdminState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
