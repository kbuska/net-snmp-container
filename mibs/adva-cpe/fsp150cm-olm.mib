--
-- OLM Session Monitor Threshold Table
--
f3OlmSessionMonitorThresholdTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF F3OlmSessionMonitorThresholdEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "A table of entries that allow manageability of OLM Session Monitor Thresholds."
    ::= { f3OlmPerformance 3 }

f3OlmSessionMonitorThresholdEntry OBJECT-TYPE
    SYNTAX      F3OlmSessionMonitorThresholdEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
            "A conceptual row in the f3OlmSessionMonitorThresholdTable."
    INDEX { neIndex, shelfIndex, slotIndex,
            f3OlmControllerIndex, f3OlmSessionMonitorIndex,
            f3OlmSessionMonitorStatsIndex, f3OlmSessionMonitorThresholdIndex }
    ::= { f3OlmSessionMonitorThresholdTable 1 }

F3OlmSessionMonitorThresholdEntry ::= SEQUENCE {
    f3OlmSessionMonitorThresholdIndex       Integer32,
    f3OlmSessionMonitorThresholdInterval    CmPmIntervalType,
    f3OlmSessionMonitorThresholdVariable    VariablePointer,
    f3OlmSessionMonitorThresholdValueLo     Unsigned32,
    f3OlmSessionMonitorThresholdValueHi     Unsigned32,
    f3OlmSessionMonitorThresholdMonValue    Counter64
}

f3OlmSessionMonitorThresholdIndex OBJECT-TYPE
    SYNTAX     Integer32 (1..65535)
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
        "An index that uniquely identifies an entry in the
        f3OlmSessionMonitorThresholdTable."
    ::= { f3OlmSessionMonitorThresholdEntry 1 }

f3OlmSessionMonitorThresholdInterval OBJECT-TYPE
    SYNTAX     CmPmIntervalType
    MAX-ACCESS read-only
    STATUS     current
    DESCRIPTION
